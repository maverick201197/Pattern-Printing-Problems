#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i,j,row,col;
    cout<<"enter row" <<endl;
    cin>>row;
    cout<<"enter column" <<endl;
    cin>>col;
    for(i=1; i<=row; i++)
    {
        for(j=1; j<=col; j++)
        {
            if(i==1 || i==row || j==1 || j==col)
            {
                cout<<"*";
            }
            else
            {
                cout<<" ";
            }
            
            
        }
        cout<<endl;
       
    } return 0;
}

enter row
5
enter column
6
******
*    *
*    *
*    *
******
